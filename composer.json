{
    "name": "obernard/property-indexer",
    "type": "library",
    "description": "Object property manipulation tools based on symfony/property-access",
    "keywords": ["Index", "Collection", "Property", "Accessor", "GroupBy", "Tree"],
    "license": "MIT",
  
    "authors": [
      {
        "name": "Olivier Bernard"
      }
    ],
  
    "require": {
      "php": ">=8.0",
        "symfony/property-access": ">=5.4"
    },
  
 
    "autoload": {
      "psr-4": {
        "Obernard\\PropertyIndexer\\": "src/"
      }
    },
    "autoload-dev": {
      "psr-4": {
        "Obernard\\PropertyIndexer\\Tests\\": "tests/"
      }
    },
    "require-dev": {
      "phpunit/phpunit": "^9.3"
    },
    "scripts": {
        "test": [
            "vendor/bin/phpunit -v"
        ]
    },
    "minimum-stability": "dev"
  }
