{
    "name": "obernard/linkedlist",
    "type": "library",
    "description": "A property indexer and accessor based on symfony/property-access for objects or arrays",
    "keywords": ["Index", "Collection", "Property", "Accessor"],
    "license": "MIT",
  
    "authors": [
      {
        "name": "Olivier Bernard"
      }
    ],
  
    "require": {
      "php": ">=8.0",
        "symfony/property-access": "6.1.x-dev"
    },
  
 
    "autoload": {
      "psr-4": {
        "Obernard\\PropertyIndexer\\": "src/"
      }
    },
    "autoload-dev": {
      "psr-4": {
        "Obernard\\PropertyIndexer\\Tests\\": "tests/"
      }
    },
    "require-dev": {
      "phpunit/phpunit": "^9.3"
    },
    "scripts": {
        "test": [
            "vendor/bin/phpunit -v"
        ]
    },
    "minimum-stability": "dev"
  }
